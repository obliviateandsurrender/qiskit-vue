{
  "name": "@qiskit-community/qiskit-ui-components",
  "version": "1.0.0",
  "description": "UI components for Qiskit community websites.",
  "main": "dist/@qiskit-community/qiskit-ui-components.ssr.js",
  "browser": "dist/@qiskit-community/qiskit-ui-components.esm.js",
  "module": "dist/@qiskit-community/qiskit-ui-components.esm.js",
  "unpkg": "dist/@qiskit-community/qiskit-ui-components.min.js",
  "types": "types.d.ts",
  "files": [
    "dist/*",
    "types.d.ts",
    "src/**/*.vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@carbon/icons-vue": "^10.24.0",
    "@carbon/type": "^10.20.0",
    "@carbon/vue": "^2.37.0",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-typescript": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "carbon-components": "^10.27.0",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "sass-loader": "^10.1.1",
    "typescript": "^3.8.3",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=10"
  }
}
